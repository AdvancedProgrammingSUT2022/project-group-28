<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="borderPane" xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="views.TechnologyPage"
            prefHeight="400.0" prefWidth="600.0"
            stylesheets="@../css/technology_page.css"
            styleClass="background">

    <center>
        <VBox styleClass="technology_vbox1" spacing="20" alignment="TOP_CENTER">
            <Button styleClass="technology_tree_button" fx:id="technologyTreePage" text="open Technology tree" onMouseClicked="#goThechnologyTreePage"/>
            <ScrollPane styleClass="technologies" >
                <VBox styleClass="technology_vbox2" alignment="CENTER"  spacing="20" fx:id="technologiesContainer">

                </VBox>
            </ScrollPane>
            <Button styleClass="back_button" fx:id="backButton" text="Back" onMouseClicked="#back"/>
        </VBox>
    </center>

</BorderPane>
